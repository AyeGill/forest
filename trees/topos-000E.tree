\begin{tikzcd}
	&& {M\hcomp N} \\
	& M && N \\
	X && Y && Z
	\arrow[from=2-2, to=3-1]
	\arrow[from=2-2, to=3-3]
	\arrow[from=2-4, to=3-3]
	\arrow[from=2-4, to=3-5]
	\arrow[from=1-3, to=2-4]
	\arrow[from=1-3, to=2-2]
